<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.web.mapper.SysUserMapper">

    <resultMap id="BaseResultMap" type="com.example.web.entity.SysUser">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="sysUserName" column="sys_user_name" jdbcType="VARCHAR"/>
            <result property="sysPassWord" column="sys_pass_word" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,sys_user_name,sys_pass_word
    </sql>
</mapper>
